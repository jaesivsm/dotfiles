[color]
    diff = auto
    status = auto
    branch = auto
    ui = true
[color "status"]
    header    = white
    added     = green
    changed   = red
    untracked = yellow
    branch    = bold white
    # never seen
    nobranch  = reverse magenta
[color "branch"]
    current  = reverse green
    local    = white
    remote   = cyan
    upstream = cyan
    # never seen
    plain    = reverse cyan
[alias]
    ci = commit
    co = checkout
    st = status
    br = branch
    wave = log --decorate --graph --color --oneline
    graph = log --decorate --graph --color
